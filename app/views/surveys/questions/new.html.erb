<%= link_to "Back to Home", surveys_path %>

<div class='container'>
  <div class='col-md-4 col-md-offset-4'>
    <h1 align='center'> <u>Aash's Survey Site</u> </h1>
    <br>
    <h4 align='center'> <b>1.Build Survery</b> | 2. Add Questions </h4>
    <p> Create your Questions</p>
    <h5> Survey Title: <%=@questionaire.title%><br>
                      Survey Description: <%=@questionaire.description%></h5>
    <h3> Add a Question:</h3>
    <p> Choose the type of question to add:</p>

    <%= form_for Question.new, url: surveys_questions_path do |question_field| %>
      Title <%= question_field.text_field :title %>
      <%= question_field.submit %>

      <%= link_to "Finished", surveys_path %>
    <%end%>

  
  </div>
</div>